
#define P_4uhvIsAliveString			"AGILENT4UHV_ISALIVE"				/* asynInt32, r, 1:connected 0:disconnected */

record(bi, "$(LOCA):$(DEV):IsAlive")
{
    field(DTYP, "asynInt32")
    field(ONAM, "normal")
    field(ZNAM, "fault")
    field(INP, "@asyn($(PORT),$(ADDR),$(TIMEOUT))AGILENT4UHV_ISALIVE")
    field(SCAN,"I/O Intr")

    info(autosaveFields, "ZSV OSV ZNAM ONAM")
}

