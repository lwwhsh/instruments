#
# OMRON E5GN temperature reading with identification string
#

record(ai, "$(P)$(R)TEMP1")
{
   field(DTYP,"stream")
   field(EGU,"degree C")
   field(INP, "@devE5GN.proto readTemp1 $(PORT)")
   field(SCAN,"2 second")
   field(ESLO,"0.1")
   field(LINR,"LINEAR")
}

