    field(FLNK, "$(P)$(R)READ_SETPOINT.PROC PP MS")
}


##### Run Sub Record for Get stat #########
record(sub,"$(P)$(R)sub")
{
    field(SCAN, "Passive")
    field(INAM, "temp_gpibSubInit")
    field(SNAM, "temp_gpibSubProcess")
    field(FLNK, "$(P)$(R)CHECK.PROC PP MS")
}

grecord(fanout,"$(P)$(R)CHECK")
{
    field(SCAN,"Passive")
    field(LNK1,"$(P)$(R)SENsts.PROC PP MS")
    field(LNK2,"$(P)$(R)ALARMsts.PROC PP MS")
    field(LNK3,"$(P)$(R)REMOTEsts.PROC PP MS")
}

