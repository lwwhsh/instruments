record(ai,"$(P)$(R)TEMP") {
    field(DESC, "$(P)$(R)Temp.")
    field(SCAN, "5 second")
    field(DTYP, "temp_gpib")
    field(PREC, "1")
    field(INP,  "#L$(L) A$(A) @0")
    field(LINR, "NO CONVERSION")
    field(EGU,  "C")
    field(HIHI, "30.0")
    field(HIGH, "29.0")
    field(LOLO, "20.0")
    field(LOW, "19.0")
    field(HHSV, "MAJOR")
    field(LLSV, "MAJOR")
    field(HSV, "MINOR")
    field(LSV, "MINOR")    
    field(AOFF, "0")
    field(ASLO, "1")
    field(SIMS, "NO_ALARM")
    field(FLNK, "$(P)$(R)sub.PROC NPP MS")
}

record(ai,"$(P)$(R)READ_SETPOINT") {
    field(DESC, "$(P)$(R)SetPoint Rx")
    field(SCAN, "Passive")
    field(DTYP, "temp_gpib")
    field(PREC, "1")
    field(INP,  "#L$(L) A$(A) @1")
    field(LINR, "NO CONVERSION")
    field(EGU,  "C")
    field(HIHI, "70.0")
    field(HIGH, "65.0")
    field(LOLO, "20.0")
    field(LOW, "19.0")
    field(HHSV, "MAJOR")
    field(LLSV, "MAJOR")
    field(HSV, "MINOR")
    field(LSV, "MINOR")    
    field(AOFF, "0")
    field(ASLO, "1")
    field(SIMS, "NO_ALARM")
    field(PINI, "YES")
}

record(ao,"$(P)$(R)WRITE_SETPOINT") {
    field(DESC, "$(P)$(R)SetPoint Tx")
    field(DTYP, "temp_gpib")
    field(OMSL, "closed_loop")
    field(PREC, "1")
    field(OUT,  "#L$(L) A$(A) @2")
    field(LINR, "NO CONVERSION")
    field(EGU,  "C")
    field(HIHI, "70.0")
    field(HIGH, "65.0")
    field(LOLO, "20.0")
    field(LOW, "19.0")
    field(HHSV, "MAJOR")
    field(LLSV, "MAJOR")
    field(HSV, "MINOR")
    field(LSV, "MINOR")    
    field(HOPR, "60.0")
